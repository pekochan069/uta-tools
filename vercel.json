{
  "redirects": [
    {
      "source": "/tools/:path*",
      "destination": "/ko/tools/:path*"
    },
    {
      "source": "/tldr/:path*",
      "destination": "/ko/tools/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(ko|en)/tools/converters/images/:path*",
      "headers": [
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        }
      ]
    }
  ]
}
