---
// import TimelineContents from "~/components/contents/tools/youtube/TimelineContents";
import Timelines from "~/components/contents/tools/youtube/TimelineContents/Timelines.astro";
import ContentLayout from "~/layouts/ContentLayout.astro";
---

<ContentLayout>
  <!-- <TimelineContents client:only /> -->
  <Timelines />
  <script is:inline>
    let tag = document.createElement("script");
    tag.src = "https://www.youtube.com/iframe_api";
    let firstScriptTag = document.getElementsByTagName("script")[0];
    firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);
  </script>
</ContentLayout>
