---
import ContentLayout from "~/layouts/ContentLayout.astro";
import HexContents from "~/components/contents/tools/converters/HexContents";

const paths = Astro.url.pathname
  .split("/")
  .filter((p) => p !== "" && p !== "tools");
const category = paths[0];
const name = paths[1];
---

<ContentLayout category={category} name={name}>
  <HexContents client:only />
</ContentLayout>
