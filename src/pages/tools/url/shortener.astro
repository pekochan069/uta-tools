---
import ContentLayout from "~/layouts/ContentLayout.astro";
import UrlShortenerContents from "~/components/contents/tools/url/ShortenerContents";

const paths = Astro.url.pathname
  .split("/")
  .filter((p) => p !== "" && p !== "tools");
const category = paths[0];
const tool = paths[1];
---

<ContentLayout category={category} name={tool}>
  <UrlShortenerContents client:only />
</ContentLayout>
