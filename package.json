{
  "name": "uta-tools",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "NODE_ENV=production astro dev",
    "build": "paraglide-js compile --project ./project.inlang --outdir ./src\\paraglide && astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "format": "prettier --write --cache .",
    "format:biome": "biome format --write .",
    "lint": "biome lint .",
    "db": "drizzle-kit",
    "db:pull": "drizzle-kit introspect:mysql",
    "db:push": "drizzle-kit push:mysql",
    "db:generate": "drizzle-kit generate:mysql",
    "postinstall": "paraglide-js compile --project ./project.inlang --outdir ./src\\paraglide",
    "vercel-json": "bun scripts/vercel.json.ts"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/solid-js": "5.0.0",
    "@astrojs/tailwind": "5.1.3",
    "@astrojs/vercel": "8.0.0",
    "@decs/typeschema": "^0.12.2",
    "@inlang/paraglide-astro": "^0.2.2",
    "@kobalte/core": "0.13.7",
    "@kobalte/tailwindcss": "^0.9.0",
    "@nanostores/solid": "0.5.0",
    "@planetscale/database": "^1.14.0",
    "@sinclair/typebox": "0.33.22",
    "@solid-primitives/upload": "0.0.117",
    "@t3-oss/env-core": "^0.11.1",
    "@tailwindcss/container-queries": "^0.1.1",
    "@thisbeyond/solid-dnd": "^0.7.5",
    "@tidbcloud/serverless": "^0.2.0",
    "@trpc/client": "next",
    "@trpc/server": "next",
    "@vercel/speed-insights": "^1.0.9",
    "astro": "5.0.3",
    "chart.js": "4.4.6",
    "class-variance-authority": "^0.7.0",
    "clsx": "2.1.1",
    "drizzle-orm": "0.36.4",
    "effect": "3.10.13",
    "nanoid": "^5.0.4",
    "nanostores": "0.11.3",
    "overlayscrollbars": "2.10.0",
    "overlayscrollbars-solid": "0.5.5",
    "solid-icons": "^1.1.0",
    "solid-js": "1.9.3",
    "solid-sonner": "0.2.8",
    "solid-spinner": "^0.2.0",
    "tailwind-merge": "2.5.4",
    "tailwindcss": "3.4.14",
    "tailwindcss-animate": "1.0.7",
    "ts-pattern": "5.5.0",
    "typescript": "5.6.3",
    "wasm-vips": "^0.0.11"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.0",
    "@inlang/paraglide-js": "1.11.3",
    "@types/bun": "^1.1.14",
    "drizzle-kit": "0.28.1",
    "mysql2": "^3.9.1",
    "prettier": "^3.2.4",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-tailwindcss": "0.6.8",
    "vercel": "39.1.1"
  }
}
